{
    "name": "kbsali/redmine-api",
    "type": "library",
    "description": "Redmine API client",
    "homepage": "https://github.com/kbsali/php-redmine-api",
    "keywords": ["redmine", "api"],
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Saliou",
